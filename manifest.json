{
  "manifest_version": 2,
  "name": "Phind Search Engine",
  "description": "Adds phind.com as search engine.",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "128": "icon-128.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Phind",
      "search_url": "https://phind.com/search?q={searchTerms}",
      "keyword": "!p, @phind",
      "favicon_url": "https://phind.com/images/favicon.png",
      "is_default": true,
      "encoding": "UTF-8"
    }
  }
}
